<p id="notice"><%= notice %></p>

<h1>Buildings</h1>

<table>
  <thead>
    <tr>
      <th>Nombre edificio</th>
      <th>Calle</th>
      <th>Numero</th>
      <th>Comuna</th>
      <th>Region</th>
      <th>Codigo postal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buildings.each do |building| %>
      <tr>
        <td><%= building.nombre_edificio %></td>
        <td><%= building.calle %></td>
        <td><%= building.numero %></td>
        <td><%= building.comuna %></td>
        <td><%= building.region %></td>
        <td><%= building.codigo_postal %></td>
        <td><%= link_to 'Show', building %></td>
        <td><%= link_to 'Edit', edit_building_path(building) %></td>
        <td><%= link_to 'Destroy', building, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Building', new_building_path %>
